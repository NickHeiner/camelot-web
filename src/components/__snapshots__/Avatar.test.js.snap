exports[`Avatar has a current user active 1`] = `"<img src=\"photoURL\" alt=\"avatar for Display Name\" class=\"profile-picture active\"/>"`;

exports[`Avatar has a current user inactive 1`] = `"<img src=\"photoURL\" alt=\"avatar for Display Name\" class=\"profile-picture \"/>"`;

exports[`Avatar renders with no user 1`] = `"<span class=\"profile-picture unknown\" aria-label=\"No one\">?</span>"`;
